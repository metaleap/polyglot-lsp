{
    "DisplayName": "Go",
    "PkgFile": "go.mod",
    "SrcFileExt": ".go",
    "BaseTypeMapping": {
        "string": "string",
        "uinteger": "uint",
        "integer": "int",
        "boolean": "bool",
        "decimal": "float64",
        "DocumentUri": "DocumentURI",
        "URI": "URI"
    },
    "Tmpls": {
        "type_Array":"[]{{.Dot.Type .Type.ElemType}}",
        "type_Map":"map[{{.Dot.Type .Type.KeyType}}]{{.Dot.Type .Type.ValueType}}"
    },
    "Dict": {},
    "PostGenTools": {
        "Format": {
            "Disabled": true,
            "Cmd": "gofmt -l -w -s -e {file}",
            "PerFile": true
        },
        "Check": [
            {
                "Cmd": "go build -trimpath -o /dev/null {files}",
                "Env": {
                    "GOCACHE": "{user_home_dir}/.cache/go-build",
                    "GO111MODULE": "off"
                }
            },
            {
                "Cmd": "go vet -C {dir}",
                "Env": {
                    "GOCACHE": "{user_home_dir}/.cache/go-build",
                    "GO111MODULE": "off"
                }
            }
        ]
    }
}
