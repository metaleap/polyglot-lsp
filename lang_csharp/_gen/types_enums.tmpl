{{- with $root := . -}}
{{- range .Items -}}
{{- with $enum := . -}}

{{- if eq $enum.Type "string" }}
{{ .DocComments $root -}}
public static class {{ $enum.Name }} {
    {{- range .Enumerants }}
    {{ .DocComments $root -}}
    public static readonly string {{ .NameCap }} = {{ .Value }};
    {{- end -}}
}
{{ else }}
{{ .DocComments $root -}}
public enum {{ $enum.Name }} {
    {{- range .Enumerants }}
    {{ .DocComments $root -}}
    {{ .NameCap }} = {{ .Value }},
    {{ end -}}
}
{{ end }}

{{- end -}}
{{- end -}}
{{- end -}}
