{
    "DisplayName": "C#",
    "PkgFile": "*.csproj",
    "SrcFileExt": ".cs",
    "PostGenTools": {
        "Format": {
            "Cmd": "dotnet format whitespace {dir} --folder",
            "Env": {
                "DOTNET_CLI_TELEMETRY_OPTOUT": "1"
            }
        },
        "Check": [
            {
                "Cmd": "dotnet build {pkg_file_path} --nologo -o {trash_dir}{pkg_file_name}",
                "Env": {
                    "DOTNET_CLI_TELEMETRY_OPTOUT": "1"
                }
            }
        ]
    },
    "PostGenCleanUp":["obj"],
    "Dict": {}
}
